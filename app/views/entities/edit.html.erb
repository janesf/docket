  </tr>
  <tr>
    <td align="right"><%= f.label :smallchecked %></td>
    <td><%= f.date_select :smallckd %></td>
  </tr>
  <tr>
    <td align="center" colspan="2"><%= f.submit 'Update' %></td>
  </tr>
<% end %>
</table>
