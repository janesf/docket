<% provide(:title, 'Editing Entity') %>
  <h1>Editing <%= @entity.name %></h1>

<p>Find me in app/views/entities/edit.html.erb</p>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@entity) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>

<table>


  <tr>
    <td align="right"><%= f.label 'entitytype' %></td>
    <td><%= f.text_field :entitytype_id %></td>
 
  </tr>
  <tr>
    <td align="right"><%= f.label :jurisdiction %></td>
    <td><%= f.text_field :jurisdiction %></td>
  </tr>
  <tr>
    <td align="right"><%= f.label :name %></td>
    <td><%= f.text_field :name %></td>
  </tr>
  <tr>
    <td align="right"><%= f.label :address1 %></td>
    <td><%= f.text_field :address1 %></td>
  </tr>
  <tr>
    <td align="right"><%= f.label :address2 %></td>
    <td><%= f.text_field :address2 %></td>
  </tr>
  <tr>
    <td align="right"><%= f.label :city %></td>
    <td><%= f.text_field :city %></td>
  </tr>
  <tr>
    <td align="right"><%= f.label :state %></td>
    <td><%= f.text_field :state %></td>
  </tr>
  <tr>
    <td align="right"><%= f.label :postalcode %></td>
    <td><%= f.text_field :postalcode %></td>
  </tr>
  <tr>
    <td align="right"><%= f.label :country %></td>
    <td><%= f.text_field :country %></td>
  </tr>
  <tr>
    <td align="right"><%= f.label :mainphone %></td>
    <td><%= f.text_field :mainphone %></td>
  </tr>
  <tr>
    <td align="right"><%= f.label :mainfax %></td>
    <td><%= f.text_field :mainfax %></td>
  </tr>
  <tr>
    <td align="right"><%= f.label :contactname %></td>
    <td><%= f.text_field :contactname %></td>
  </tr>
  <tr>
    <td align="right"><%= f.label :contactphone %></td>
    <td><%= f.text_field :contactphone %></td>
  </tr>
  <tr>
    <td align="right"><%= f.label :contactfax %></td>
