<h1>Listing Country Codes</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Ccode</th>
    <th>Cname</th>
  </tr>

<% @countrycodes.each do |countrycode| %>
  <tr>
    <td><%=h countrycode.id %></td>
    <td><%=h countrycode.ccode %></td>
    <td><%=h countrycode.cname %></td>
    <td><%= link_to 'Show', countrycode %></td>
    <td><%= link_to 'Edit', edit_countrycode_path(countrycode) %></td>
    <td><%= link_to 'Destroy', countrycode, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New countrycode', new_countrycode_path %>